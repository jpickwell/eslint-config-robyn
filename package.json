{
  "name": "eslint-config-robyn",
  "version": "2.0.0",
  "description": "Robyn Style - Shareable Configs",
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/jpickwell/eslint-config-robyn.git"
  },
  "homepage": "https://gitlab.com/jpickwell/eslint-config-robyn#readme",
  "bugs": {
    "url": "https://gitlab.com/jpickwell/eslint-config-robyn/issues"
  },
  "author": "Jordan Pickwell",
  "license": "MIT",
  "private": false,
  "engines": {
    "node": ">= 10"
  },
  "main": "index.js",
  "keywords": [
    "config",
    "eslint",
    "lint",
    "markdown",
    "php",
    "prettier",
    "robyn",
    "ruby",
    "standard",
    "standard style",
    "style",
    "stylelint",
    "unicorn",
    "xml"
  ],
  "scripts": {
    "clean": "rm -fr node_modules pnpm-lock.yaml",
    "format": "prettier --ignore-unknown --write '*'",
    "format:check": "prettier --ignore-unknown --check '*'",
    "install:clean": "pnpm run clean && pnpm i",
    "lint": "eslint --cache -f friendly --report-unused-disable-directives .",
    "lint:config": "eslint --print-config index.js",
    "pnpm:update": "npm i -g pnpm",
    "prepare": "husky install",
    "release": "standard-version -s",
    "test": "pnpm run lint && pnpm run test:jest",
    "test:jest": "jest --coverage"
  },
  "dependencies": {
    "@prettier/plugin-php": "^0.17.2",
    "@prettier/plugin-ruby": "^1.6.1",
    "@prettier/plugin-xml": "^0.13.1",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-lodash": "^7.2.0",
    "eslint-plugin-markdown": "^2.2.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-unicorn": "^34.0.1",
    "eslint-plugin-vue": "^7.13.0",
    "eslint-plugin-you-dont-need-lodash-underscore": "^6.12.0",
    "eslint-plugin-you-dont-need-momentjs": "^1.6.0",
    "linguist-languages": "^7.15.0",
    "lodash": "^4.17.21",
    "prettier": "^2.3.2",
    "prettier-plugin-pkg": "^0.10.0",
    "prettier-plugin-sh": "^0.7.1",
    "prettier-plugin-sql": "^0.3.0",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-prettier": "^1.2.0"
  },
  "devDependencies": {
    "eslint-formatter-friendly": "^7.0.0",
    "eslint-plugin-jest": "^24.3.6",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "standard-version": "^9.3.0",
    "typescript": "^4.3.5"
  },
  "lint-staged": {
    "*": "prettier --write"
  }
}
