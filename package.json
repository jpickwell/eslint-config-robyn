{
	"name": "eslint-config-robyn",
	"version": "4.1.3",
	"type": "commonjs",
	"description": "Robyn Style - Shareable Configs",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/jpickwell/eslint-config-robyn.git"
	},
	"homepage": "https://github.com/jpickwell/eslint-config-robyn#readme",
	"bugs": {
		"url": "https://github.com/jpickwell/eslint-config-robyn/issues"
	},
	"author": "Jordan Pickwell",
	"license": "MIT",
	"private": false,
	"engines": {
		"node": ">= 16"
	},
	"main": "index.cjs",
	"keywords": [
		"check",
		"checker",
		"code",
		"code checker",
		"code linter",
		"code standards",
		"code style",
		"comment",
		"config",
		"enforce",
		"eslint",
		"eslintconfig",
		"hint",
		"import",
		"jscs",
		"jshint",
		"lint",
		"lodash",
		"markdown",
		"node",
		"php",
		"policy",
		"prettier",
		"promise",
		"quality",
		"robyn",
		"ruby",
		"scoped css",
		"security",
		"shell",
		"simple",
		"sql",
		"style",
		"style checker",
		"style linter",
		"stylelint",
		"TypeScript",
		"unicorn",
		"verify",
		"vue",
		"xml",
		"you-dont-need"
	],
	"scripts": {
		"clean": "rm -fr node_modules pnpm-lock.yaml",
		"eslint": "eslint -f pretty --color --max-warnings 0 --no-error-on-unmatched-pattern",
		"eslint:config": "eslint --print-config",
		"format": "pnpm prettier -w '{**/.*,.*,**/*,*}'",
		"format:check": "pnpm prettier:check '{**/.*,.*,**/*,*}'",
		"ignore-sync": "ignore-sync .",
		"install:clean": "pnpm clean && pnpm i",
		"lint": "echo 'Linting ...'; pnpm --silent eslint '{**/.*,.*,**/*,*}.{c[jt]s,html,js?(x),md,m[jt]s,ts?(x),vue}'",
		"lint:config": "pnpm eslint:config index.cjs",
		"prepare": "scripts/is-dependency.cjs || is-ci || husky install",
		"prettier": "prettier -u",
		"prettier:check": "pnpm prettier -c",
		"release": "standard-version -s",
		"test": "scripts/test.sh"
	},
	"peerDependencies": {
		"vue-eslint-parser": "^9.0.2"
	},
	"peerDependenciesMeta": {
		"vue-eslint-parser": {
			"optional": true
		}
	},
	"dependencies": {
		"@prettier/plugin-php": "^0.18.8",
		"@prettier/plugin-ruby": "^3.1.2",
		"@prettier/plugin-xml": "^2.2.0",
		"@typescript-eslint/eslint-plugin": "^5.29.0",
		"@typescript-eslint/parser": "^5.29.0",
		"confusing-browser-globals": "^1.0.11",
		"eslint": "^8.18.0",
		"eslint-formatter-pretty": "^4.1.0",
		"eslint-import-resolver-typescript": "^2.7.1",
		"eslint-plugin-ava": "^13.2.0",
		"eslint-plugin-eslint-comments": "^3.2.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-lodash": "^7.4.0",
		"eslint-plugin-markdown": "^2.2.1",
		"eslint-plugin-n": "^15.2.3",
		"eslint-plugin-no-use-extend-native": "^0.5.0",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-promise": "^6.0.0",
		"eslint-plugin-security": "^1.5.0",
		"eslint-plugin-simple-import-sort": "^7.0.0",
		"eslint-plugin-unicorn": "^42.0.0",
		"eslint-plugin-vue": "^9.1.1",
		"eslint-plugin-you-dont-need-lodash-underscore": "^6.12.0",
		"glob": "^8.0.3",
		"globals": "^13.15.0",
		"linguist-languages": "^7.21.0",
		"lodash": "^4.17.21",
		"prettier": "^2.7.1",
		"prettier-plugin-pkg": "^0.14.0",
		"prettier-plugin-sh": "^0.10.2",
		"prettier-plugin-sql": "^0.8.0",
		"read-pkg-up": "^7.0.1",
		"typescript": "^4.7.4"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.0.2",
		"@commitlint/config-conventional": "^17.0.2",
		"@linthtml/linthtml": "^0.8.6",
		"@types/eslint": "^8.4.3",
		"eslint-config-prettier": "^8.5.0",
		"eslint-find-rules": "^4.1.0",
		"html-validate": "^7.1.1",
		"html-validate-vue": "^5.1.0",
		"husky": "^8.0.1",
		"ignore-sync": "^5.0.1",
		"is-ci": "^3.0.1",
		"lint-staged": "^13.0.2",
		"postcss": "^8.4.14",
		"postcss-html": "^1.4.1",
		"postcss-scss": "^4.0.4",
		"standard-version": "^9.5.0",
		"stylelint": "^14.9.1",
		"stylelint-config-html": "^1.0.0",
		"stylelint-config-prettier": "^9.0.3",
		"stylelint-config-recommended": "^8.0.0",
		"stylelint-config-recommended-scss": "^6.0.0",
		"stylelint-config-recommended-vue": "^1.4.0",
		"stylelint-config-standard": "^26.0.0",
		"stylelint-config-standard-scss": "^4.0.0",
		"stylelint-config-standard-vue": "^1.0.0",
		"stylelint-order": "^5.0.0",
		"stylelint-prettier": "^2.0.0",
		"stylelint-scss": "^4.2.0",
		"synchronized-promise": "^0.3.1",
		"vue-eslint-parser": "^9.0.2"
	}
}
