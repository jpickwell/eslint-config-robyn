{
	"name": "eslint-config-robyn",
	"version": "6.4.0",
	"type": "commonjs",
	"description": "Robyn Style - Shareable Configs",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/jpickwell/eslint-config-robyn.git"
	},
	"homepage": "https://github.com/jpickwell/eslint-config-robyn#readme",
	"bugs": {
		"url": "https://github.com/jpickwell/eslint-config-robyn/issues"
	},
	"author": "Jordan Pickwell",
	"license": "MIT",
	"engines": {
		"node": ">= 16"
	},
	"main": "configs/index.js",
	"keywords": [
		"check",
		"checker",
		"code",
		"code checker",
		"code linter",
		"code standards",
		"code style",
		"comment",
		"config",
		"enforce",
		"eslint",
		"eslintconfig",
		"hint",
		"import",
		"jscs",
		"jshint",
		"lint",
		"lodash",
		"markdown",
		"node",
		"php",
		"policy",
		"prettier",
		"promise",
		"quality",
		"robyn",
		"ruby",
		"scoped css",
		"security",
		"shell",
		"simple",
		"sql",
		"style",
		"style checker",
		"style linter",
		"stylelint",
		"TypeScript",
		"unicorn",
		"verify",
		"vue",
		"xml",
		"you-dont-need"
	],
	"scripts": {
		"check:deprecated-rules": "scripts/build-full-config.js && tests/deprecated.js",
		"clean": "rm -fr node_modules package-lock.json",
		"eslint": "eslint -f pretty --color --no-error-on-unmatched-pattern",
		"eslint:config": "eslint --print-config",
		"eslint:fix": "npm run eslint -- --fix",
		"format": "npm run prettier:write -- .",
		"format:check": "npm run prettier:check -- .",
		"ignore-sync": "ignore-sync .",
		"install:clean": "npm run clean && npm install",
		"lint": "echo 'Linting ...'; npm run eslint -- --ext .js,.md,.ts,.vue .",
		"lint:config": "npm run eslint:config -- configs/index.js",
		"postinstall": "node scripts/is-dependency.js || is-ci || husky install",
		"prettier": "prettier -u",
		"prettier:check": "npm run prettier -- -c",
		"prettier:write": "npm run prettier -- -w",
		"release": "bin/release.sh",
		"test": "scripts/test.sh"
	},
	"peerDependencies": {
		"vue-eslint-parser": "^9.0.2"
	},
	"peerDependenciesMeta": {
		"vue-eslint-parser": {
			"optional": true
		}
	},
	"dependencies": {
		"@prettier/plugin-php": "^0.19.1",
		"@prettier/plugin-ruby": "^3.2.2",
		"@prettier/plugin-xml": "^2.2.0",
		"@typescript-eslint/eslint-plugin": "^5.40.0",
		"@typescript-eslint/parser": "^5.40.0",
		"confusing-browser-globals": "^1.0.11",
		"eslint": "^8.25.0",
		"eslint-formatter-pretty": "^4.1.0",
		"eslint-import-resolver-typescript": "^3.5.1",
		"eslint-plugin-ava": "^13.2.0",
		"eslint-plugin-eslint-comments": "^3.2.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-lodash": "^7.4.0",
		"eslint-plugin-markdown": "^3.0.0",
		"eslint-plugin-n": "^15.3.0",
		"eslint-plugin-no-use-extend-native": "^0.5.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-promise": "^6.0.1",
		"eslint-plugin-security": "^1.5.0",
		"eslint-plugin-unicorn": "^44.0.2",
		"eslint-plugin-vue": "^9.6.0",
		"eslint-plugin-you-dont-need-lodash-underscore": "^6.12.0",
		"glob": "^8.0.3",
		"globals": "^13.17.0",
		"linguist-languages": "^7.21.0",
		"lodash": "^4.17.21",
		"prettier": "^2.7.1",
		"prettier-plugin-pkg": "^0.17.1",
		"prettier-plugin-sh": "^0.10.2",
		"prettier-plugin-sql": "^0.12.1",
		"read-pkg-up": "^7.0.1",
		"typescript": "^4.8.4"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.1.2",
		"@commitlint/config-conventional": "^17.1.0",
		"@linthtml/linthtml": "^0.9.3",
		"@types/eslint": "^8.4.6",
		"eslint-config-prettier": "^8.5.0",
		"eslint-find-rules": "^4.1.0",
		"html-validate": "^7.6.0",
		"html-validate-vue": "^5.1.1",
		"husky": "^8.0.1",
		"ignore-sync": "^6.0.2",
		"is-ci": "^3.0.1",
		"lint-staged": "^13.0.3",
		"node-gyp": "^9.3.0",
		"pinst": "^3.0.0",
		"postcss": "^8.4.17",
		"postcss-html": "^1.5.0",
		"postcss-scss": "^4.0.5",
		"standard-version": "^9.5.0",
		"stylelint": "^14.13.0",
		"stylelint-config-html": "^1.1.0",
		"stylelint-config-prettier": "^9.0.3",
		"stylelint-config-recommended": "^9.0.0",
		"stylelint-config-recommended-scss": "^7.0.0",
		"stylelint-config-recommended-vue": "^1.4.0",
		"stylelint-config-standard": "^28.0.0",
		"stylelint-config-standard-scss": "^5.0.0",
		"stylelint-config-standard-vue": "^1.0.0",
		"stylelint-order": "^5.0.0",
		"stylelint-prettier": "^2.0.0",
		"stylelint-scss": "^4.3.0",
		"synchronized-promise": "^0.3.1",
		"vue-eslint-parser": "^9.1.0"
	}
}
