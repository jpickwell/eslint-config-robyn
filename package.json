{
  "name": "eslint-config-robyn",
  "version": "1.3.0",
  "private": false,
  "description": "Robyn Style - Shareable Configs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/jpickwell/eslint-config-robyn.git"
  },
  "author": "Jordan Pickwell",
  "homepage": "https://gitlab.com/jpickwell/eslint-config-robyn#readme",
  "bugs": {
    "url": "https://gitlab.com/jpickwell/eslint-config-robyn/issues"
  },
  "main": "index.js",
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "clean": "rm -fr node_modules pnpm-lock.yaml",
    "format": "prettier --write *",
    "format:check": "prettier --check *",
    "install:clean": "pnpm run clean && pnpm i",
    "lint": "eslint --cache -f friendly --report-unused-disable-directives .",
    "release": "standard-version -s",
    "test": "pnpm run lint && pnpm run test:jest",
    "test:jest": "jest --coverage"
  },
  "keywords": [
    "config",
    "eslint",
    "markdown",
    "php",
    "prettier",
    "robyn",
    "ruby",
    "standard",
    "stylelint",
    "unicorn",
    "xml"
  ],
  "dependencies": {
    "@prettier/plugin-php": "^0.14.3",
    "@prettier/plugin-ruby": "^0.20.0",
    "@prettier/plugin-xml": "^0.12.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.8.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-markdown": "^1.0.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-unicorn": "^21.0.0",
    "linguist-languages": "^7.10.0",
    "prettier": "^2.1.1",
    "prettier-plugin-package": "^1.0.0",
    "stylelint": "^13.7.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-prettier": "^1.1.2"
  },
  "devDependencies": {
    "eslint-formatter-friendly": "^7.0.0",
    "eslint-plugin-jest": "^23.20.0",
    "husky": "^4.2.5",
    "jest": "^26.4.2",
    "lint-staged": "^10.2.13",
    "standard-version": "^9.0.0",
    "typescript": "^4.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*": "prettier --write"
  }
}
